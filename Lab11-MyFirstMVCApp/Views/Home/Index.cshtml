<body>
    <div class="search">
        <h1>TIME</h1>
        <h4>People of the Year</h4>
        <form method="POST">
            <div class="begYear">
                <label for="begYear">Beginning Year:</label>
                <input maxlength="12" name="begYear" />
            </div>
            <div>
                <label for="endYear">Ending Year:</label>
                <input name="endYear" />
            </div>
            <div>
                <button type="submit">Search Influential People</button>
            </div>
        </form>
    </div>
</body>